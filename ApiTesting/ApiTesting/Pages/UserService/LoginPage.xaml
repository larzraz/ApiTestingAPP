<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ApiTesting.Pages.LoginPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Text="{Binding Username}" Placeholder="Brugernavn"/>
            <Entry Text="{Binding Password}" Placeholder="Kodeord" IsPassword="True"/>
            <Button Text="registrer"/>
            <Button Text="Login" Clicked="LoginUser" />
            

        </StackLayout>
    </ContentPage.Content>
</ContentPage>